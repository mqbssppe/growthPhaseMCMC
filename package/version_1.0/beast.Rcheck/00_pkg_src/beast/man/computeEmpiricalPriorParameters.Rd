\name{computeEmpiricalPriorParameters}
\alias{computeEmpiricalPriorParameters}
\title{
Compute the empirical mean.
}
\description{
This function computes the empirical mean of the time-series.
}
\usage{
computeEmpiricalPriorParameters(myDataList, nu0 = 1, alpha0 = 1, beta0 = 1)
}
\arguments{
  \item{myDataList}{
Observed multivariate time-series.
}
  \item{nu0}{
positive real number.
}
  \item{alpha0}{
positive real number.
}
  \item{beta0}{
positive real number.
}
}
\value{
 \item{mu0}{Empirical mean}
}
\author{
Panagiotis Papastamoulis
}

